{% set field_content %}
  {% for item in items %}
    {% if item.clean %}
      {{ item.content }}
    {% else %}
      <{{ item.wrapper_attribute }}{{ item.attributes }}>{{ item.content }}</{{ item.wrapper_attribute }}>
    {% endif %}
  {% endfor %}
{% endset %}

{% if clean %}
  {{ field_content }}
{% else %}
  <{{ wrapper_attribute }}{{ attributes }}>{{ field_content }}</{{ wrapper_attribute }}>
{% endif %}
